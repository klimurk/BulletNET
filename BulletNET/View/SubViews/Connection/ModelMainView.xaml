<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:uc="clr-namespace:BulletNET.View.SubViews"
                    xmlns:vm="clr-namespace:BulletNET.ViewModels.SubView">

    <DataTemplate DataType="{x:Type vm:DashBoardViewModel}">
        <uc:DashboardView DataContext="{Binding }" />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:HistoryViewModel}">
        <uc:HistoryView DataContext="{Binding }" />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AdminViewModel}">
        <uc:AdminView DataContext="{Binding }" />
    </DataTemplate>
</ResourceDictionary>